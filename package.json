{
  "name": "wakeupcoach",
  "version": "0.0.1",
  "description": "A simple project for a good wake up",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node app/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --ignore ./public app/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "react-scripts build",
    "deploy": "gcloud app deploy -q",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
       "patterns": [ "src" ],
         "extensions": "js,jsx"
       }
  },
