    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^6.0.2",
    "express-session": "^1.17.2",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "mongoose": "^6.1.4",
    "morgan": "^1.10.0",
    "nedb-async": "^0.1.6",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "react-redux": "^7.2.6",
    "react-scripts": "^5.0.0"
  },
  "browserslist": {
    "production": [
